msgid "Game"
msgstr "Խաղ"

#: PathsActivity.py:180
msgid "Start a new game."
msgstr "Սկսել նոր խաղ"

#: PathsActivity.py:184
msgid "Play with the robot."
msgstr "Խաղալ ռոբոտի հետ"

#: PathsActivity.py:193
msgid "Turn complete"
msgstr "Հերթը ամբողջությամբ է"

